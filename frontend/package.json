{
  "name": "mega-event",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test": "concurrently \"npm run dev\" \"npm run cypress:run\"",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint \"{cypress,src}/**/*.{js,ts,tsx}\"",
    "eslint:fix": "eslint \"{cypress,src}/**/*.{js,ts,tsx}\" --fix",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "stylelint:fix": "stylelint \"src/**/*.scss\" --fix",
    "prettier": "prettier \"{cypress,src}/**/*.{js,ts,tsx,json}\" --write",
    "prettier:styles": "prettier \"src/**/*.scss\" --write",
    "format": "npm run eslint:fix && stylelint:fix && prettier && prettier:styles",
    "reinstall-modules": "rm -rf ./node_modules && npm i"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.8.2",
    "classnames": "2.5.1",
    "date-fns": "4.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "9.2.0",
    "react-router-dom": "6.30.1",
    "sass": "1.90.0"
  },
  "devDependencies": {
    "@types/cypress": "1.1.6",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitejs/plugin-react": "4.7.0",
    "concurrently": "9.2.0",
    "cypress": "13.17.0",
    "cypress-vite": "1.6.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.2",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-cypress": "2.15.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "6.6.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "4.6.2",
    "lefthook": "1.12.3",
    "prettier": "3.6.2",
    "stylelint": "16.23.1",
    "stylelint-config-idiomatic-order": "10.0.0",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-order": "7.0.0",
    "stylelint-scss": "6.12.1",
    "typescript": "5.9.2",
    "vite": "5.4.19"
  }
}
