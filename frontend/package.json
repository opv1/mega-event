{
  "name": "mega-event",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test": "concurrently \"npm run dev\" \"npm run cypress:run\"",
    "tsc": "tsc --noEmit",
    "eslint": "eslint \"{cypress,src}/**/*.{js,ts,tsx}\"",
    "eslint:fix": "eslint \"{cypress,src}/**/*.{js,ts,tsx}\" --fix",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "stylelint:fix": "stylelint \"src/**/*.scss\" --fix",
    "prettier": "prettier \"{cypress,src}/**/*.{js,ts,tsx,json}\" --write",
    "prettier:styles": "prettier \"src/**/*.scss\" --write",
    "format": "npm run eslint:fix && stylelint:fix && prettier && prettier:styles",
    "reinstall-modules": "rm -rf ./node_modules && npm i"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.2.2",
    "classnames": "2.5.1",
    "date-fns": "3.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "9.1.0",
    "react-router-dom": "6.22.3",
    "sass": "1.72.0"
  },
  "devDependencies": {
    "@million/lint": "0.0.73",
    "@react-dev-inspector/vite-plugin": "2.0.1",
    "@types/cypress": "1.1.3",
    "@types/react": "18.2.73",
    "@types/react-dom": "18.2.22",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "@vitejs/plugin-react": "4.2.1",
    "concurrently": "8.2.2",
    "cypress": "13.7.1",
    "cypress-vite": "1.5.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "lefthook": "1.6.7",
    "million": "3.0.6",
    "prettier": "3.2.5",
    "react-dev-inspector": "2.0.1",
    "stylelint": "16.3.1",
    "stylelint-config-idiomatic-order": "10.0.0",
    "stylelint-config-standard-scss": "13.0.0",
    "stylelint-order": "6.0.4",
    "stylelint-scss": "6.2.1",
    "typescript": "5.4.3",
    "vite": "5.2.6"
  }
}
